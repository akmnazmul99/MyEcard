<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape android:shape="rectangle">
            <padding android:left="6dp"
                android:top="6dp"
                android:right="6dp"
                android:bottom="6dp"/>
            <gradient
                android:startColor="#E5E5E5"
                android:centerColor="#bfbfb2"
                android:endColor="#555555"
                android:angle="270"
                />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle">
            <padding android:left="6dp"
                android:top="6dp"
                android:right="6dp"
                android:bottom="6dp"/>
            <solid android:color="#F76902"/>
        </shape>
    </item>
    <item >

        <shape android:shape="rectangle">
            <gradient
            android:startColor="#49474C"
            android:centerColor="#333334"
            android:endColor="#111111"
            android:angle="-90"/>
        </shape>
    </item>
</layer-list>

